{
    "ace": {
        "@guessTabSize": "true",
        "@newLineMode": "unix",
        "@tabSize": "4",
        "@useSoftTabs": "true"
    },
    "build": {
        "@builder": "auto",
        "@path": "/.c9/builders",
        "@saveall": true
    },
    "find.nak": {
        "@searchLimit": 100
    },
    "language": {
        "@instanceHighlight": "true",
        "@semi": "true",
        "@undeclaredVars": "true",
        "@unusedFunctionArgs": "false",
        "@warnLevel": "info"
    },
    "run": {
        "@path": "/.c9/runners",
        "configs": {
            "@inited": "true",
            "json()": {
                "Ruby on Rails": {
                    "command": "",
                    "default": true,
                    "name": "Ruby on Rails",
                    "runner": "Ruby on Rails (Rails 4.0 + Ruby 2.1)"
                },
                "start rails": {
                    "command": "rails s -p $PORT -b $IP",
                    "debug": true,
                    "name": "start rails",
                    "runner": "Shell command"
                }
            }
        }
    },
    "share": {
        "@app": false,
        "@preview": false,
        "@useOwnerSettings": false
    }
}